{
    "name": "didymo/service-project-base",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0+",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "package",
            "package": {
                "name": "rotaryuow/service_club_base",
                "version": "8.0.0",
                "type": "drupal-profile",
                "source": {
                    "url": "https://github.com/rotaryuow/service_club_base.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "rotaryuow/bt_subtheme",
                "version": "8.0.0",
                "type": "drupal-theme",
                "source": {
                    "url": "https://github.com/rotaryuow/bt_subtheme",
                    "type": "git",
                    "reference": "master"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "rotaryuow/assetmanage",
                "version": "8.0.0",
                "type": "drupal-module",
                "source": {
                    "url": "https://github.com/rotaryuow/assetmanage",
                    "type": "git",
                    "reference": "master"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "rotaryuow/userprofiles",
                "version": "8.0.0",
                "type": "drupal-module",
                "source": {
                    "url": "https://github.com/rotaryuow/userprofiles",
                    "type": "git",
                    "reference": "master"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "rotaryuow/manage_event",
                "version": "8.0.0",
                "type": "drupal-module",
                "source": {
                    "url": "https://github.com/rotaryuow/manage_event",
                    "type": "git",
                    "reference": "master"
                }
            }
        }
    ],
    "require": {
        "ext-curl": "*",
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6",
        "drupal/calendar": "*",
        "drupal-composer/drupal-scaffold": "^2.2",
        "drupal/console": "^1.0.1",
        "drupal/core": "~8.0",
        "drupal/commerce": "~2.0",
        "drupal/swiftmailer": "~1.0",
        "drupal/token": "~1.0",
        "drupal/bootstrap": "~3.0",
        "drupal/admin_toolbar": "~1.0",
        "drupal/address_map_link": "~1.0",
        "drupal/bootstrap_layouts": "~5.0",
        "drupal/courier": "~1.0",
        "drupal/ctools": "~3.0",
        "drupal/ds": "~3.0",
        "drupal/dynamic_entity_reference": "~1.0",
        "drupal/flag": "~4.0",
        "drupal/front": "~1.0",
        "drupal/geolocation": "~1.0",
        "drupal/message": "~1.0",
        "drupal/message_notify": "~1.0",
        "drupal/message_subscribe": "~1.0",
        "drupal/panels": "~4.0",
        "drupal/pathauto": "~1.0",
        "drupal/rng": "~1.0",
        "drupal/rng_conflict": "~1.0",
        "drupal/rng_contact": "~1.0",
        "drupal/rng_quick": "~1.0",
        "drupal/rng_date_scheduler": "~1.0",
        "drupal/rules": "3.0-alpha3",
        "drupal/scheduler": "~1.0",
        "drupal/simplify": "~1.0",
        "drupal/typed_data": "~1.0",
        "drupal/unlimited_number": "~1.0",
        "drupal/veoa": "~1.0",
        "drupal/views_advanced_routing": "~1.0",
        "drupal/views_templates": "~1.0",
        "drupal/phpmailer": "~3.0",
        "drupal/shorten": "~1.0",
        "phpmailer/phpmailer": "~5.0",
        "drush/drush": "~9.0",
        "rotaryuow/service_club_base": "*",
        "rotaryuow/bt_subtheme": "*",
        "rotaryuow/assetmanage": "*",
        "rotaryuow/userprofiles": "*",
        "rotaryuow/manage_event": "*",
        "webflo/drupal-finder": "^1.0.0",
        "webmozart/path-util": "^2.3"
    },
    "require-dev": {
        "behat/mink": "1.7.x-dev",
        "behat/mink-goutte-driver": "~1.2",
        "jcalderonzumba/gastonjs": "~1.0.2",
        "jcalderonzumba/mink-phantomjs-driver": "~0.3.1",
        "mikey179/vfsstream": "~1.2",
        "phpunit/phpunit": ">=4.8.28 <5",
        "symfony/css-selector": "~3.2.8"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ]
    },
    "scripts": {
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/contrib/{$name}": ["type:drupal-drush"]
        },
        "patches": {
        	"drupal/rules": {
        		"Rules type definition": "https://www.drupal.org/files/issues/2694685-52.rules-fix_type_definition_error.patch",
                "Rules type definition": "rules_send_email_to_roles.diff"
        	},
            "drupal/calendar": {
                "Rules type definition": "Calendar-timezone.diff"
            }
        }
    }
}
